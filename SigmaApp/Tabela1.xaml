<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="SigmaApp.Tabela1"
             xmlns:local="clr-namespace:SigmaApp">

    <NavigationPage.TitleView>
        <StackLayout Orientation="Horizontal">
            <Label Text="NBR ISO 13857 - Tabela 3" TextColor="#03A64A" VerticalTextAlignment="Center"/>
            <Image Source="icoSigma" HorizontalOptions="EndAndExpand" HeightRequest="50" Margin="0,0,20,0"/>
        </StackLayout>
    </NavigationPage.TitleView>

    <local:PinchPanContainer >
        <local:PinchPanContainer.Content>
            <Image Source="Tab3" VerticalOptions="Start" HeightRequest="600" Margin="10,0,10,0"/>
        </local:PinchPanContainer.Content>
    </local:PinchPanContainer>

</ContentPage>
    